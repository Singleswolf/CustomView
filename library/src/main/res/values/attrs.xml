<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--ArcRotate start-->
    <attr name="animationDur" format="float"/>
    <declare-styleable name="ArcRotate">
        <attr name="startAngle" format="float"/>
        <attr name="sweepAngle" format="float"/>
        <!-- 圆弧宽度 -->
        <attr name="arcWidth" format="dimension"/>
        <attr name="arcColor" format="color|reference"/>
        <attr name="animationDur"/>
    </declare-styleable>
    <!--ArcRotate end-->

    <!--LoadingView start-->
    <declare-styleable name="LoadingView">
        <!--间隔-->
        <attr name="interval" format="dimension"/>
        <attr name="circleNum" format="integer"/>
        <attr name="radius" format="dimension"/>
        <attr name="animationDur"/>
    </declare-styleable>
    <!--LoadingView end-->
</resources>