<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--ArcRotate start-->
    <attr name="startAngle" format="float"/>
    <attr name="sweepAngle" format="float"/>
    <!-- 圆弧宽度 -->
    <attr name="arcWidth" format="dimension" />
    <attr name="arcColor" format="color|reference"/>
    <declare-styleable name="ArcRotate">
        <attr name="startAngle"/>
        <attr name="sweepAngle"/>
        <attr name="arcColor"/>
        <attr name="animationDur"/>
        <attr name="arcWidth"/>
    </declare-styleable>
    <!--ArcRotate end-->

    <!--LoadingView start-->
    <!--间隔-->
    <attr name="interval" format="dimension"/>
    <attr name="circleNum" format="integer"/>
    <attr name="radius" format="dimension"/>
    <declare-styleable name="LoadingView">
        <attr name="interval"/>
        <attr name="circleNum"/>
        <attr name="radius"/>
        <attr name="animationDur"/>
    </declare-styleable>
    <!--LoadingView end-->
</resources>